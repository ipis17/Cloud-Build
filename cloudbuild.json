{
    "steps": [
    {
      "name": "launcher.gcr.io/google/ubuntu1604",
      "id": "Create GitHub pull request",
      "entrypoint": "bash",
      "args": [
        "-c",
         "curl -X POST -H \"Authorization:Bearer $$GH_TOKEN\" -H 'Accept:application/vnd.github.v3+json' https://api.github.com/repos/ipis17/Cloud-Build -d '{\"head\":\"master\",\"base\":\"dev\", \"title\":\"new_pull\"}"
      ],
      "secretEnv": ["GH_TOKEN"]
  }
  ],
  "availableSecrets": {
    "secretManager": [
    {
      "versionName": "projects/test-darwin-328107/secrets/github_token/versions/latest",
      "env": "GH_TOKEN"
    }
    ]
  }
  }